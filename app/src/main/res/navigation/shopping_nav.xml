<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/shopping_nav"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.devsstore.devsstore.ui.fragment.shopping.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.devsstore.devsstore.ui.fragment.shopping.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/sofaSetFragment"
        android:name="com.devsstore.devsstore.ui.fragment.shopping.SofaSetFragment"
        android:label="fragment_sofa_set"
        tools:layout="@layout/fragment_sofa_set" />
    <fragment
        android:id="@+id/profilesFragment"
        android:name="com.devsstore.devsstore.ui.fragment.profile.ProfilesFragment"
        android:label="fragment_profiles"
        tools:layout="@layout/fragment_profiles" />
</navigation>